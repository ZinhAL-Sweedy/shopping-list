<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.name_field :name %>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :shoppingList_id %>
  <%= f.shoppingList_id_field :shoppingList_id %>
  <br>
  <%= f.submit %>
<% end %>
