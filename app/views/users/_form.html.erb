<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f._field :name %>
  <br>
  <%= f.label :email %>
  <%= f.label_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.label_field :password %>
  <br>
  <%= f.label :shoppingList_id %>
  <%= f.label_field :shoppingList_id %>
  <br>
  <%= f.submit %>
<% end %>
